{
  "names_regex": {
    "start": [
      "(nome|sono|io)"
    ],
    "rules": [
      "/^[A-Z][a-z0-9_-]{3,19}$/"
    ]
  },
  "names": {
    "start": [],
    "rules": [
      "#name"
    ]
  },
  "emails": {
    "start": [],
    "rules": [
      "#email"
    ]
  },
  "numbers": {
    "start": [],
    "rules": [
      "#number"
    ]
  },
  "dates": {
    "start": [],
    "rules": [
      "#date"
    ]
  },
  "phones": {
    "start": [
      "(numero|cell|cellulare|telefono|chiama|richiamarmi|chiama**|chiama***)"
    ],
    "rules": [
      "#phone"
    ]
  },
  "multiple": {
    "start": [],
    "rules": [
      "#integerLen#5",
      "#startsWithExp#VEN-#([0-9a-zA-Z-])"
    ]
  },
  "regexp": {
    "start": [],
    "rules": [
      "#exp#([a-zA-Z]{2})+([0-9]{8})"
    ]
  }
}